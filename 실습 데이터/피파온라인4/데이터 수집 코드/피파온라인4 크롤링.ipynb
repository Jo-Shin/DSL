   "source": [
    "import pickle\n",
    "import pandas as pd\n",
    "\n",
    "with open(\"./매치아이디.txt\", \"rb\") as file:\n",
    "    match_ids = pickle.load(file)\n",
    "    \n",
    "with open(\"./선수정보.txt\", \"rb\") as file2:\n",
    "    player_dict = pickle.load(file2)"
   ]
